{
    "title": "MyNatsClient - async, reactive and simple NATS client",
    "description": "Provides a simple, effective sync and async library for interacting with NATS Server using .NET and .NET Core. It uses IObservable so it is ReactiveX (RX) friendly.",
    "authors": [ "danielwertheim" ],
    "version": "0.0.0-*",
    "copyright": "danielwertheim",
    "buildOptions": {
        "platform": "anycpu",
        "nowarn": [ "CS1591" ],
        "compile": {
            "includeFiles": [ "../../GlobalAssemblyInfo.cs" ]
        }
    },
    "configurations": {
        "Release": {
            "buildOptions": {
                "optimize": true,
                "xmlDoc": true
            }
        }
    },
    "packOptions": {
        "summary": "NATS Server client for .NET and .NET Core.",
        "tags": [ "NATS", "NATS.io", "messaging", "PubSub", "pub-sub", "request-response", "rx", "reactivex", "reactiveextensions" ],
        "owners": [ "danielwertheim" ],
        "projectUrl": "https://github.com/danielwertheim/mynatsclient",
        "licenseUrl": "http://www.opensource.org/licenses/MIT",
        "requireLicenseAcceptance": false,
        "repository": {
            "type": "git",
            "url": "https://github.com/danielwertheim/mynatsclient"
        },
        "releaseNotes": "https://github.com/danielwertheim/mynatsclient/blob/master/ReleaseNotes.md"
    },
    "dependencies": {
        "Ensure.That": "5.0.0"
    },
    "frameworks": {
        "netstandard1.6": {
            "buildOptions": {
                "define": [ "DOTNETCORE" ]
            },
            "dependencies": {
                "NETStandard.Library": "1.6.0"
            }
        },
        "net45": {
            "buildOptions": {
                "define": [ "NET" ]
            }
        }
    }
}
