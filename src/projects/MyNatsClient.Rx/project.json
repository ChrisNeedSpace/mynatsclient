{
    "title": "MyNatsClient.Rx async and reactive NATS client",
    "description": "Convenience package that installs MyNatsClient and ReactiveExtensions (System.Reactive). MyNatsClient provides a simple, effective sync and async library for interacting with NATS Server using .NET and .NET Core.",
    "authors": [ "danielwertheim" ],
    "version": "0.0.0-*",
    "copyright": "danielwertheim",
    "buildOptions": {
        "platform": "anycpu",
        "compile": {
            "includeFiles": [ "../../GlobalAssemblyInfo.cs" ]
        }
    },
    "packOptions": {
        "summary": "NATS Server client for .NET and .NET Core with RX support.",
        "tags": [ "NATS", "NATS.io", "messaging", "PubSub", "pub-sub", "request-response", "rx", "reactivex", "reactiveextensions" ],
        "owners": [ "danielwertheim" ],
        "projectUrl": "https://github.com/danielwertheim/mynatsclient",
        "licenseUrl": "http://www.opensource.org/licenses/MIT",
        "requireLicenseAcceptance": false,
        "repository": {
            "type": "git",
            "url": "https://github.com/danielwertheim/mynatsclient"
        },
        "releaseNotes": "https://github.com/danielwertheim/mynatsclient/blob/master/ReleaseNotes.md",
        "files": "README.md"
    },
    "dependencies": {
        "MyNatsClient": "*",
        "System.Reactive": "3.1.1"
    },
    "frameworks": {
        "netstandard1.6": {
            "dependencies": {
                "NETStandard.Library": "1.6.0"
            }
        },
        "net45": {
        }
    }
}
