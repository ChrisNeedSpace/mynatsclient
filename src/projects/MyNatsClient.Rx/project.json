{
    "title": "MyNatsClient.Rx - async, reactive and simple NATS.io client",
    "description": "Convenience package that installs MyNatsClient and ReactiveExtensions (System.Reactive). MyNatsClient provides a simple, effective sync and async library for interacting with NATS.io using DotNet. Uses IObservable so it is ReactiveX friendly.",
    "authors": [ "danielwertheim" ],
    "version": "0.0.0-*",
    "copyright": "danielwertheim",
    "buildOptions": {
        "platform": "anycpu",
        "compile": {
            "includeFiles": [ "../../GlobalAssemblyInfo.cs" ]
        }
    },
    "packOptions": {
        "summary": "Used for working with NATS.io message broker with ReactiveExtensions support.",
        "tags": [ "NATS", "NATS.io", "messaging", "rx", "reactivex", "reactiveextensions" ],
        "owners": [ "danielwertheim" ],
        "projectUrl": "https://github.com/danielwertheim/mynatsclient",
        "licenseUrl": "http://www.opensource.org/licenses/MIT",
        "requireLicenseAcceptance": false,
        "repository": {
            "type": "git",
            "url": "https://github.com/danielwertheim/mynatsclient"
        },
        "releaseNotes": "https://github.com/danielwertheim/mynatsclient/blob/master/ReleaseNotes.md",
        "files": "README.md"
    },
    "frameworks": {
        "netstandard1.6": {
            "dependencies": {
                "NETStandard.Library": "1.6.0",
                "MyNatsClient": "*",
                "System.Reactive": "3.0.0"
            }
        },
        "net45": {
            "dependencies": {
                "MyNatsClient": "*",
                "System.Reactive": "3.0.0"
            }
        }
    }
}
